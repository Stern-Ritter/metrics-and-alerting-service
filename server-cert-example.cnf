[ req ]
default_bits       = 4096
default_md         = sha256
prompt             = no
encrypt_key        = no
default_keyfile    = server-key.pem
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
C  = RU
ST = Russia
L  = Moscow
O  = DEV
OU = DEV
CN = localhost

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost